{
	"compilerOptions": {
		"lib": ["ESNext"],

		"module": "ESNext",
		"target": "ESNext",

		"noEmit": true,
		"moduleDetection": "force",
		"moduleResolution": "Bundler",
		"esModuleInterop": true,

		"skipLibCheck": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noPropertyAccessFromIndexSignature": true,
		"exactOptionalPropertyTypes": true,
		"noImplicitOverride": true
	},
	"include": ["types"]
}
